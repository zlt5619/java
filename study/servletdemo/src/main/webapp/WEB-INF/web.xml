<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <context-param>
        <param-name>key</param-name>
        <param-value>value</param-value>
    </context-param>
    <context-param>
        <param-name>key2</param-name>
        <param-value>value2</param-value>
    </context-param>
    <servlet>
        <servlet-name>GetRequestDataServlet</servlet-name>
        <servlet-class>com.example.servletdemo.GetRequestDataServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetRequestDataServlet</servlet-name>
        <url-pattern>/getrequestdata</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetHeaderServlet</servlet-name>
        <servlet-class>com.example.servletdemo.GetHeaderServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetHeaderServlet</servlet-name>
        <url-pattern>/getheader</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LanguageServlet</servlet-name>
        <servlet-class>com.example.servletdemo.LanguageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LanguageServlet</servlet-name>
        <url-pattern>/language</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ResponseCharactorServlet</servlet-name>
        <servlet-class>com.example.servletdemo.ResponseCharactorServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ResponseCharactorServlet</servlet-name>
        <url-pattern>/ResponseCharactor</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ResponseByteServlet</servlet-name>
        <servlet-class>com.example.servletdemo.ResponseByteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ResponseByteServlet</servlet-name>
        <url-pattern>/ResponseByte</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DownloadFileServlet</servlet-name>
        <servlet-class>com.example.servletdemo.DownloadFileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DownloadFileServlet</servlet-name>
        <url-pattern>/DownloadFile</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletContextServlet</servlet-name>
        <servlet-class>com.example.servletdemo.ServletContextServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletContextServlet</servlet-name>
        <url-pattern>/ServletContext</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServeltConfigServlet</servlet-name>
        <servlet-class>com.example.servletdemo.ServeltConfigServlet</servlet-class>
        <init-param>
            <param-name>key</param-name>
            <param-value>value</param-value>
        </init-param>
        <init-param>
            <param-name>key1</param-name>
            <param-value>value1</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServeltConfigServlet</servlet-name>

        <url-pattern>/ServeltConfig</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CookieServlet</servlet-name>
        <servlet-class>com.example.servletdemo.CookieServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CookieServlet</servlet-name>
        <url-pattern>/Cookie</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CookieServlet02</servlet-name>
        <servlet-class>com.example.servletdemo.CookieServlet02</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CookieServlet02</servlet-name>
        <url-pattern>/Cookie02</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SessionServlet</servlet-name>
        <servlet-class>com.example.servletdemo.SessionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SessionServlet</servlet-name>
        <url-pattern>/Session</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AutoServlet</servlet-name>
        <servlet-class>com.example.servletdemo.AutoServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>AutoServlet</servlet-name>
        <url-pattern>/auto</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ThreadSafeServlet</servlet-name>
        <servlet-class>com.example.servletdemo.ThreadSafeServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>ThreadSafeServlet</servlet-name>
        <url-pattern>/ThreadSafe</url-pattern>
    </servlet-mapping>
    
    <filter>
        <filter-name>FirstFilter</filter-name>
        <filter-class>com.example.filter.FirstFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>FirstFilter</filter-name>
        <url-pattern>/suibian/*</url-pattern>
    </filter-mapping>

    <filter>
        <filter-name>EncodingFilter</filter-name>
        <filter-class>com.example.filter.EncodingFilter</filter-class>
        <init-param>
            <param-name>code</param-name>
            <param-value>utf-8</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>EncodingFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    
    <listener>
        <listener-class>com.example.Listener.ServerContextLifeCycleListener</listener-class>
    </listener>
</web-app>